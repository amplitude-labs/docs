{
  "openapi": "3.1.0",
  "info": { "title": "FastAPI", "version": "0.1.0" },
  "paths": {
    "/api/extract": {
      "post": {
        "summary": "Accept File And Extract",
        "operationId": "accept_file_and_extract_api_extract_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": { "$ref": "#/components/schemas/ExtractForm" }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/ping": {
      "get": {
        "summary": "Ping Pong",
        "operationId": "ping_pong_ping_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ContentTypeFilter": {
        "properties": {
          "include": {
            "type": "boolean",
            "title": "Include",
            "default": false
          },
          "content_types": {
            "items": {
              "type": "string",
              "enum": ["table", "image", "text", "hyperlink"]
            },
            "type": "array",
            "title": "Content Types"
          }
        },
        "type": "object",
        "required": ["content_types"],
        "title": "ContentTypeFilter"
      },
      "CustomFilter": {
        "properties": {
          "include": {
            "type": "boolean",
            "title": "Include",
            "default": false
          },
          "query": { "type": "string", "title": "Query" }
        },
        "type": "object",
        "required": ["query"],
        "title": "CustomFilter"
      },
      "ExtractForm": {
        "properties": {
          "url": { "type": "string", "title": "Url" },
          "filters": {
            "items": {
              "anyOf": [
                { "$ref": "#/components/schemas/ContentTypeFilter" },
                { "$ref": "#/components/schemas/CustomFilter" },
                { "$ref": "#/components/schemas/KeywordFilter" },
                { "$ref": "#/components/schemas/PageNumberFilter" },
                { "$ref": "#/components/schemas/PageOrientationFilter" }
              ]
            },
            "type": "array",
            "title": "Filters"
          }
        },
        "type": "object",
        "required": ["url", "filters"],
        "title": "ExtractForm"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "KeywordFilter": {
        "properties": {
          "include": {
            "type": "boolean",
            "title": "Include",
            "default": false
          },
          "keywords": {
            "items": { "type": "string" },
            "type": "array",
            "title": "Keywords"
          }
        },
        "type": "object",
        "required": ["keywords"],
        "title": "KeywordFilter"
      },
      "PageNumberFilter": {
        "properties": {
          "include": {
            "type": "boolean",
            "title": "Include",
            "default": false
          },
          "pages": {
            "items": { "type": "integer" },
            "type": "array",
            "title": "Pages"
          }
        },
        "type": "object",
        "required": ["pages"],
        "title": "PageNumberFilter"
      },
      "PageOrientationFilter": {
        "properties": {
          "include": {
            "type": "boolean",
            "title": "Include",
            "default": false
          },
          "orientation": {
            "type": "string",
            "enum": ["portrait", "landscape"],
            "title": "Orientation"
          }
        },
        "type": "object",
        "required": ["orientation"],
        "title": "PageOrientationFilter"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    }
  }
}
